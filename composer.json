{
  "name": "tsrgtm/db-manager",
  "description": "A Laravel package for database visualization and CRUD operations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tusar Gautam",
      "email": "mailme@tusargautam.com.np",
      "homepage": "https://tusargautam.com.np"
    }
  ],
  "require": {
    "php": "^8.1.0",
    "laravel/framework": "^11.31",
    "livewire/livewire": "^3.5"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.13",
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^11.0.1"
  },
  "autoload": {
    "psr-4": {
      "Tsrgtm\\DbManager\\": "src/"
